POS = game.Players.LocalPlayer.Character.Head.CFrame
local function SavePos(POS)
    pcall(function()
        POS = game.Players.LocalPlayer.Character.Head.CFrame;
    end);
        SavedPosition = POS;
    end;
function LoadPos()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = (SavedPosition)
end;
local Humr = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
pickup = true
while pickup == true do
if game.workspace:FindFirstChild("Diamond") then
    SavePos()
    Humr.CFrame = game:GetService("Workspace").Diamond.Primary.CFrame
    wait(.1)
local args = {
    [1] = workspace.Diamond,
    [2] = "Pickup"
}

game:GetService("ReplicatedStorage").References.Comm.Events.ItemInteracted:FireServer(unpack(args))
 wait(.1)
 LoadPos()
end
if game.workspace:FindFirstChild("Ruby Boots") then
    SavePos()
    Humr.CFrame = game:GetService("Workspace"):FindFirstChild("Ruby Boots").MainWeld.CFrame
    wait(.1)
local args = {
    [1] = workspace:FindFirstChild("Ruby Boots"),
    [2] = "Pickup"
}

