crim = true
guard = false
neutral = false
prisioner = false
neutral1 = false
POS = game.Players.LocalPlayer.Character.Head.CFrame

local function SavePos(POS)
    pcall(function()
        POS = game.Players.LocalPlayer.Character.Head.CFrame;
    end);
        SavedPosition = POS;
    end;

function LoadPos()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = (SavedPosition)
repeat
wait()
until
game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
end;
getgenv().c = false
getgenv().c = true
while getgenv().c == true do
if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
SavePos()
wait(.1)
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = (game.Players.localPlayer.Name)
}

workspace.Remote.loadchar:InvokeServer(unpack(args))
wait()
	LCS = game.Workspace["Criminals Spawn"].SpawnLocation

