local Humr = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
local function attack()
Humr.CFrame = game:GetService("Workspace").MobHolder["King slime (Lv. 5)"].Torso.CFrame
wait(.3)
for i=1,25 do
local args = {
    [1] = workspace.MobHolder:FindFirstChild("King slime (Lv. 5)").Torso,
    [2] = workspace.MobHolder:FindFirstChild("King slime (Lv. 5)").Enemy,
    [3] = game:GetService("Players").LocalPlayer.Character:FindFirstChild("Copper Shortsword")
}

game:GetService("ReplicatedStorage").GameRemotes.DamageEvent:FireServer(unpack(args))
end
wait(.3)
end


