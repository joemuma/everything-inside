if game.Players.LocalPlayer ~= game.Players.maneleer67120 then
local Players = game:GetService("Players")

local helpcommand = ":h"
local freezeCommand = ":freeze"
local unfreezeCommand = ":unfreeze"
local ResetCommand = ":r"
local KickCommand = ":kick"
local admins = {
    "maneleer67120"
 }
local function isAdmin(player)

   for _, v in pairs(admins) do
       if v == player.Name then
           return true
       end
   end

   return false

end


local function onPlayerChatted(player, message, recipient)
    if isAdmin(player) then
        if message:sub(1, helpcommand:len()):lower() == helpcommand:lower() then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
        end
        if message:sub(1, freezeCommand:len()):lower() == freezeCommand:lower() then
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
        end
        if message:sub(1, unfreezeCommand:len()):lower() == unfreezeCommand:lower() then
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
        end
        if message:sub(1, KickCommand:len()):lower() == KickCommand:lower() then
            game.Players.LocalPlayer:Kick("\nGet Kicked\n\nReason:Owner didnt want u in here")
        end
        if message:sub(1, ResetCommand:len()):lower() == ResetCommand:lower() then
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
        end
    end
end
local function onPlayerAdded(player)
    player.Chatted:Connect(function (...)
       onPlayerChatted(player, ...)
   end)
end

for _, player in pairs(Players:GetPlayers()) do
   onPlayerAdded(player)
end
Players.PlayerAdded:Connect(onPlayerAdded)
end
